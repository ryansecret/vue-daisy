{
  "name": "vue-daisy",
  "displayName": "vue-daisy",
  "description": "vue enhance extension for elementUI、@jdcloud/jelement-biz、@jdcloud/dbU",
  "version": "1.13.1",
  "publisher": "ryansecreat",
  "repository": {
    "url": "https://github.com/ryansecret/vue-daisy"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onCommand:vue-daisy.functionCompletion"
  ],
  "main": "./dist/extension.js",
  "icon": "logo.png",
  "contributes": {
    "commands": [
      {
        "command": "vue-daisy.ryan",
        "title": "Hello ryan"
      }
    ],
    "keybindings": [
      {
        "command": "vue-daisy.functionCompletion",
        "key": "shift+alt+enter",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/element.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/iview2.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/vue.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/iview-html.json"
      },
      {
        "language": "html",
        "path": "./snippets/element-html.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/element-html.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue-html.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Vue Daisy",
      "properties": {
        "vue-daisy.alias": {
          "type": "object",
          "default": {
            "@": "src"
          },
          "description": "webpack alias"
        },
        "vue-daisy.componentIgnore": {
          "type": "array",
          "description": "assign which dir ignore search componennt",
          "items": {
            "type": "string"
          }
        },
        "vue-daisy.componentPath": {
          "type": "array",
          "description": "assign which dir to search component",
          "items": {
            "type": "string"
          }
        },
        "vue-daisy.componentDirRgex": {
          "type": "string",
          "description": "component dir regex to search",
          "default": ".*/components/.*"
        },
        "vue-daisy.componentPrefix": {
          "type": "object",
          "description": "assign prefix to replace in the file path",
          "default": {
            "alias": "@",
            "path": "client/src"
          }
        },
        "vue-daisy.tagNameWay": {
          "description": "dictate the rule of vue component name in html section",
          "type": "string",
          "default": "CamelCase",
          "enum": [
            "kebabCase",
            "camelCase",
            "CamelCase"
          ]
        },
        "vue-daisy.indent-size": {
          "type": "number",
          "default": 2,
          "description": "Indentation size of snippets"
        },
        "vue-daisy.quotes": {
          "type": "string",
          "default": "double",
          "description": "Normal quotes of autocomplete",
          "enum": [
            "single",
            "double"
          ]
        }
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "release": "esno ./scripts/release.mts",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.18.53",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cz-conventional-changelog": "^2.1.0",
    "cz-git": "^1.3.4",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "lru-cache": "^7.12.0",
    "mocha": "^8.4.0",
    "pretty": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "esno": "^0.17.0",
    "kolorist": "^1.8.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "zx": "^7.2.3"
  },
  "volta": {
    "node": "16.18.0"
  }
}